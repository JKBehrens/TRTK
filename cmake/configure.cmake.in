set(TRTK_INCLUDE ${MEDITEC_LIBS_PATH}/@PROJECT_NAME@/@FULL_VERSION@/include )
set(TRTK_LIBDIR ${MEDITEC_LIBS_PATH}/@PROJECT_NAME@/@FULL_VERSION@/bin )
